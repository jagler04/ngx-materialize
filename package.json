{
  "name": "ngx-materialize",
  "description": "An Angular wrap around Materialize library",
  "license": "Apache-2.0",
  "repository": "sherweb/ngx-materialize",
  "keywords": [
    "angular",
    "ng",
    "ngx",
    "material",
    "material-design",
    "materialize",
    "materializecss",
    "component",
    "ui"
  ],
  "scripts": {
    "ng": "ng",
    "start": "yarn link:lib && ng serve",
    "build:demo": "ng build demo",
    "build:lib": "ng build lib",
    "postbuild:lib": "cpx ./README.md ./dist/lib",
    "link:lib": "rimraf node_modules/ngx-materialize && linklocal",
    "lint:ci": "ng lint lib --format checkstyle > tslint.xml",
    "lint:demo": "ng lint demo",
    "lint:lib": "ng lint lib",
    "test": "ng test",
    "postinstall": "typings install",
    "gh-pages:build": "yarn build:demo --prod --aot --base-href /ngx-materialize/",
    "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"CircleCI\" --email=\"circleci@email.com\" --no-silent",
    "heroku-postbuild": "yarn install && yarn build:demo --prod --aot",
    "publish:lib": "npm publish ./dist/lib"
  },
  "dependencies": {
    "@angular/animations": "^10.2.2",
    "@angular/common": "~10.2.2",
    "@angular/compiler": "~10.2.2",
    "@angular/core": "~10.2.2",
    "@angular/forms": "~10.2.2",
    "@angular/localize": "^10.0.14",
    "@angular/platform-browser": "~10.2.2",
    "@angular/platform-browser-dynamic": "~10.2.2",
    "@angular/router": "~10.2.2",
    "@mdi/font": "^5.8.55",
    "@types/jquery": "^3.5.4",
    "@types/materialize-css": "^1.0.8",
    "core-js": "^3.6.5",
    "highlightjs": "^9.10.0",
    "jquery": "^3.5.1",
    "ngx-malihu-scrollbar": "^9.0.0",
    "ngx-markdown": "^10.1.1",
    "ngx-materialize": "file:./lib",
    "rxjs": "~6.6.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.2",
    "@angular/language-service": "~10.2.2",
    "@types/jasmine": "~3.6.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~14.14.6",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "~6.0.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "linklocal": "^2.8.2",
    "ng-packagr": "^10.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "~9.0.0",
    "tsickle": ">=0.25.5",
    "tslib": "^2.0.3",
    "tslint": "~6.1.3",
    "tslint-eslint-rules": "^5.2.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "~4.0.5",
    "typings": "^2.0.0"
  }
}
